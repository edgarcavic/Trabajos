package ejercicios_clase;

public class Baby {
	char sex;
	int numDiapers;
	String name;
	double weight;
	
	Baby(String babyName, char babySex, int numDiapers, double babyweight){
		this.sex = babySex;
		this.numDiapers = numDiapers;
		this.name = babyName;
		this.weight = babyweight;
	}
	public void cry() {
		System.out.println("niania");
	}
	public void eat() {
		System.out.println("chomchom");
	}
	public void sleep() {
		System.out.println("zzzzz");
	}
	
}
